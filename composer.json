{
    "name": "ryanwinchester/carbonize",
    "description": "Carbon helper for cloning and creating new Carbon instances.",
    "type": "package",
    "license": "MIT",
    "authors": [{
        "name": "Ryan Winchester",
        "email": "code@ryanwinchester.ca"
    }],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^5.6|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.4",
        "codeclimate/php-test-reporter": "^0.4.4",
        "nesbot/carbon": "1.21.*"
    },
    "autoload": {
      "psr-4": {"Carbonize\\": "src/"},
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {"Carbonize\\Tests\\": "tests/"}
    }
}
